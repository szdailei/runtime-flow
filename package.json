{
  "name": "runtime-flow",
  "version": "0.1.0",
  "description": "Generate flow chart when code runs",
  "author": "Dai Lei <szdailei@gmail.com>",
  "keywords": [
    "flowchart",
    "runtime"
  ],
  "license": "MIT",
  "repository": "github:szdailei/runtime-flow",
  "homepage": "https://github.com/szdailei/runtime-flow#readme",
  "main": "server.js",
  "type": "module",
  "bin": {
    "runtime-flow": "cli.js",
    "runtime-flow-svg": "cli-svg.js"
  },
  "files": [
    "dist/web/",
    "dist/server.js",
    "server.js",
    "svg.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write packages/**/* src/**/*",
    "lint": "eslint packages src --ext .js,.jsx,.mjs",
    "stylelint": "stylelint --syntax \"css-in-js\" \"packages/client/src/**/*.{jsx,tsx}\"",
    "clean": "rimraf dist/",
    "build": "cross-env NODE_ENV=production node scripts/rollup/cli-build.js",
    "build:dev": "cross-env NODE_ENV=development node scripts/rollup/cli-build.js",
    "build:debug": "node scripts/rollup/cli-build-debug.js",
    "debug": "DEBUG=* node dist/debug/cli-debug.js",
    "chart": "node debug/cli-flow-chart.js",
    "server": "node cli.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest && chromium reports/jest_html_reporters.html",
    "release-info": "node scripts/publish/gen-release-note.js",
    "to-github": "node scripts/publish/to-github.js",
    "to-npm": "node scripts/publish/to-npm.js"
  },
  "dependencies": {
    "@stitches/react": "^1.2.5",
    "dagre": "^0.8.5",
    "history": "^5.0.1",
    "loglevel": "^1.7.1",
    "minimist": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@iarna/toml": "^2.2.5",
    "@octokit/rest": "^18.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "commitizen": "^4.2.4",
    "conventional-changelog": "^3.1.24",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "jest-html-reporters": "^2.1.6",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "pkg-dir": "^6.0.1",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "react-flow-renderer": "^9.6.9",
    "read-pkg-up": "^9.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
